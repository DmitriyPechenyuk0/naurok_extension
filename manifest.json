{
  "manifest_version": 3,
  "name": "Специально для тестов Акимова",
  "version": "2.0",
  "description": "by github.com/dmitriypechenyuk0",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}